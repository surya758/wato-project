{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@screens/*": ["app/screens/*"],
      "@routes/*": ["app/routes/*"],
      "@assets/*": ["app/assets/*"],
      "@themes": ["app/themes"],
      "@atoms/*": ["app/components/atoms/*"],
      "@molecules/*": ["app/components/molecules/*"],
      "@organisms/*": ["app/components/organisms/*"],
    }
  }
}
